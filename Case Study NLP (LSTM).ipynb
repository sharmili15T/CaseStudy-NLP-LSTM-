   "source": [
    "from tensorflow.keras.layers import Dropout"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 62,
   "id": "06d91837",
   "metadata": {},
   "outputs": [],
   "source": [
    "model = Sequential()\n",
    "\n",
    "model.add(Embedding(input_dim=10000, output_dim=50,input_length= seq_length))\n",
    "\n",
    "model.add(LSTM(100))\n",
    "\n",
    "#adding dropout layer \n",
    "model.add(Dropout(0.2, seed= 100))\n",
    "\n",
    "model.add(Dense(3, activation=\"softmax\"))"
   ]
